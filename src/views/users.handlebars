<h1>User List</h1>
    <table>
        <thead>
            <tr>
                <th>First Name</th>
                <th>Last Name</th>
                <th>Email</th>
                <th>Age</th>
                <th>Role</th>
                <th>Last Connection</th>
                <th>Documents</th>
                <th>Created At</th>
                <th>Updated At</th>
            </tr>
        </thead>
        <tbody>
            {{#each users}}
            <tr>
                <td>{{this.first_name}}</td>
                <td>{{this.last_name}}</td>
                <td>{{this.email}}</td>
                <td>{{this.age}}</td>
                <td>{{this.role}}</td>
                <td>{{this.last_connection}}</td>
                <td>
                    <ul>
                        {{#each this.documents}}
                        <li>{{this.name}}: {{this.reference}}</li>
                        {{/each}}
                    </ul>
                </td>
                <td>{{this.createdAt}}</td>
                <td>{{this.updatedAt}}</td>
            </tr>
            {{/each}}
        </tbody>
    </table>